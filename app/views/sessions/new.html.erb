<% provide(:title,'LogIN') %>
<h1>LOGIN</h1>
<div class="container">
  <div class="jumbotron">

    <%= form_for(:session , url: login_path) do |f| %>
    <form>
    
      <div class="form-group">
          <%= f.label :email %>
          <%= f.email_field :email, class: 'form-control' %>
        </div>

        <div class="form-group">
            <%= f.label :password %>
            <%= f.password_field :password, class: 'form-control' %>
        </div>
          <%= f.submit 'Login', class: 'btn btn-default' %>
      </form>
    <% end %>

  </div>
</div>
